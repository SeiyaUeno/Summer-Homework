   "source": [
    "xs = map(float, input().split())\n",
    "ys = map(float, input().split())\n",
    "abs_d = [abs(x - y) for (x, y) in zip(xs, ys)]\n",
    "d = [sum([i ** p for i in abs_d]) ** (1 / p) for p in range(1, 4)]\n",
    "d.append(max(abs_d))\n",
    "\n",
    "print(*d, sep='\\n')"
   ]
