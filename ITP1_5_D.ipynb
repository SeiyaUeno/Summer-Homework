   "source": [
    "n = int(input())\n",
    "list = []\n",
    "for i in range(3, n + 1):\n",
    "    if i % 3 == 0:\n",
    "        list.append(i)\n",
    "    else:\n",
    "        x = i\n",
    "        while x:\n",
    "            if x % 10 == 3:\n",
    "                list.append(i)\n",
    "                break\n",
    "            else:\n",
    "                x = x // 10\n",
    "print(\"\", *list)"
   ]
