    "while True:\n",
    "    n = int(input())\n",
    "    if n == 0:\n",
    "        break\n",
    "\n",
    "    s = list(map(float, input().split()))\n",
    "    m = sum(s) / n\n",
    "\n",
    "    a_2 = sum([(x - m) ** 2 for x in s]) / n\n",
    "    print(a_2 ** 0.5)"
