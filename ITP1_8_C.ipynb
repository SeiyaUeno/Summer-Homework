   "source": [
    "import sys\n",
    "s = sys.stdin.read().lower()\n",
    "a = {chr(i): 0 for i in range(ord('a'), ord('z')+1)}\n",
    "for i in s:\n",
    "    if ord(i) in range(ord('a'), ord('z')+1):\n",
    "        a[i] += 1\n",
    "[print(f'{k} : {v}') for k, v in a.items()]"
